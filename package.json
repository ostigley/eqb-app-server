{
  "name": "exquisite-bodies-server-module",
  "version": "1.0.0",
  "description": "Server for hidden doodle phone app",
  "engines": {
    "node": "v7.4.0"
  },
  "main": "./dist/index.js",
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "start": "node ./dist/index.js",
    "start:mongo": "nodemon ./server/testing-mongo.js --exec babel-node",
    "start:dev": "nodemon ./server/index.js --exec babel-node",
    "start:load": "nodemon ./load-test.js --exec babel-node",
    "build": "babel server -d dist",
    "watch": "watchify client/index.js -o client/public/bundle.js -dv -t babelify",
    "test:unit": "mocha test/server/unit/**.js --compilers js:babel-core/register",
    "test:db": "clear && mocha test/server/unit/database-helpers-test --compilers js:babel-core/register",
    "test:integration": "mocha test/server/integration/**.js --compilers js:babel-core/register",
    "test:all": "mocha test/server/**/*.* --compilers js:babel-core/register"
  },
  "author": "Oliver Stigley",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "flow-bin": "^0.40.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.10.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "canvas": "^1.4.0",
    "clone": "^1.0.2",
    "deep-freeze": "0.0.1",
    "deep-object-diff": "^1.0.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.24",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  }
}
